 color: grey;\n    \n`\nconst ArticleBody = styled.p`\n    width: 60%;\n    padding: 50px;\n    font-size: 20px;\n    margin: auto;\n    font-weight: lighter;\n    font-family: 'Montserrat', sans-serif;;\n`\n\nclass SingleArticle extends Component {\n\n    state = {\n        article: {},\n        loading: true,\n        hasError: false,\n        errorMessage: '',\n        deleted: false\n    }\n\n    componentDidMount() {\n        const { article_id } = this.props\n        fetchArticleById(article_id).then(article => {\n            this.setState({article, loading: false})\n        }).catch((err) => {\n            const { status, statusText } = err.response\n            this.setState({hasError: true, loading: false, errorMessage: `ERROR - Status ${status}: ${statusText}`})\n        })\n    }\n\n    deleteArticle = (id) => {\n        const answer = window.confirm(\"Delete Article?\")\n        if (answer) {\n            deleteArticle(id).then(() => {\n                this.setState({  \n                    article: {},\n                    loading: false,\n                    hasError: false,\n                    errorMessage: '',\n                    deleted: true})\n            })\n        }\n    }\n\n    navigateToHome = () => {\n        setTimeout(() => {\n            navigate('/')}, 2000)\n    \n    }\n\n    render() {\n        const { article, loading, hasError, errorMessage, deleted } = this.state\n        const { article_id } = this.props\n\n        if (loading) {\n            return (\n                <PacmanLoader\n                  css={override}\n                  size={100}\n                  color={\"#DF3B57\"}\n                  loading={this.state.loading}\n                />\n            )\n        } else if (hasError) {\n            return <ErrorMessage errorMessage={errorMessage}/>\n        } else if (deleted) {\n            return (\n                <>\n            <p>Article Has Been Deleted. Redirecting...</p>\n            {this.navigateToHome()}\n            </>\n            )\n        } else {\n            return (\n                <>\n                <ArticleHolder>\n                    <ArticleTitle>{article.title.toUpperCase()}</ArticleTitle>\n                    <p>Topic: {article.topic}</p>\n                    <ArticleBody>{article.body}</ArticleBody>\n                    <p>By {article.author}</p>\n                    <Voter votes={article.votes} id={article_id} type=\"article\"/>\n                    {(article.author === 'cooljmessy' ? <button onClick={() => this.deleteArticle(article.article_id)}>Delete Article</button> : <p></p>)}\n                </ArticleHolder>\n                <Comments id={article_id}/>\n                </>\n            );\n        }\n    }\n}\n\nexport default SingleArticle;","/home/leahjwoodhead/my-news-app/src/components/ErrorMessage.js",[],"/home/leahjwoodhead/my-news-app/src/components/Comments.js",[],"/home/leahjwoodhead/my-news-app/src/components/CommentCard.js",[],"/home/leahjwoodhead/my-news-app/src/components/PostComment.js",[],"/home/leahjwoodhead/my-news-app/src/components/Voter.js",[],"/home/leahjwoodhead/my-news-app/src/components/SortCommentsForm.js",[],"/home/leahjwoodhead/my-news-app/src/components/Footer.js",[],"/home/leahjwoodhead/my-news-app/src/components/PostArticle.js",["80"],"/home/leahjwoodhead/my-news-app/src/components/SortArticles.js",[],"/home/leahjwoodhead/my-news-app/src/components/Homepage.js",["81"],{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":4,"column":10,"nodeType":"88","messageId":"89","endLine":4,"endColumn":23},{"ruleId":"86","severity":1,"message":"90","line":9,"column":9,"nodeType":"88","messageId":"89","endLine":9,"endColumn":13},{"ruleId":"86","severity":1,"message":"90","line":4,"column":9,"nodeType":"88","messageId":"89","endLine":4,"endColumn":13},{"ruleId":"86","severity":1,"message":"91","line":4,"column":8,"nodeType":"88","messageId":"89","endLine":4,"endC