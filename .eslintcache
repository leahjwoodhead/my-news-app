ents/Articles.js",[],"/home/leahjwoodhead/my-news-app/src/components/api.js",[],"/home/leahjwoodhead/my-news-app/src/components/ArticleCard.js",[],"/home/leahjwoodhead/my-news-app/src/components/SingleArticle.js",["72","73"],"import React, { Component } from 'react';\nimport { fetchArticleById, deleteArticle } from './api'\nimport { css } from \"@emotion/core\";\nimport PacmanLoader from 'react-spinners/PacmanLoader'\nimport ErrorMessage from './ErrorMessage'\nimport styled from 'styled-components'\nimport Comments from './Comments'\nimport Voter from './Voter'\n\nconst override = css`\n  display: block;\n  margin: 0 auto;\n  border-color: red;\n`;\n\nconst ArticleHolder = styled.article`\n    border: 2px solid orange;\n    border-radius: 10px;\n    margin: 3vw 10vw;\n    height: auto;\n    background-color: white;\n`\n\nconst ArticleTitle = styled.h1`\n    font-size: 2vw;\n    color: grey;\n    \n`\nconst ArticleBody = styled.p`\n    width: 60%;\n    padding: 50px;\n    font-size: 20px;\n    margin: auto;\n    font-weight: lighter;\n    font-family: 'Montserrat', sans-serif;;\n`\n\nclass SingleArticle extends Component {\n\n    state = {\n        article: {},\n        loading: true,\n        hasError: false,\n        errorMessage: '',\n        // deleted: false\n    }\n\n    componentDidMount() {\n        const { article_id } = this.props\n        fetchArticleById(article_id).then(article => {\n            this.setState({article, loading: false})\n        }).catch((err) => {\n            const { status, statusText } = err.response\n            this.setState({hasError: true, loading: false, errorMessage: `ERROR - Status ${status}: ${statusText}`})\n        })\n    }\n\n    // deleteArticle = (id) => {\n    //     deleteArticle(id).then(() => {\n    //         console.log(\"HERE\")\n    //         this.setState({deleted: true})\n    //     })\n    // }\n\n    render() {\n        const { article, loading, hasError, errorMessage, deleted } = this.state\n        const { article_id } = this.props\n\n        if (loading) {\n            return (\n                <PacmanLoader\n                  css={override}\n                  size={100}\n                  color={\"#DF3B57\"}\n                  loading={this.state.loading}\n                />\n            )\n        } else if (hasError) {\n            return <ErrorMessage errorMessage={errorMessage}/>\n        // } else if (deleted) {\n        //     <p>Article Has Been Deleted</p>\n        } else {\n            return (\n                <>\n                <ArticleHolder>\n                    <ArticleTitle>{article.title.toUpperCase()}</ArticleTitle>\n                    <p>Topic: {article.topic}</p>\n                    <ArticleBody>{article.body}</ArticleBody>\n                    <p>By {article.author}</p>\n                    <Voter votes={article.votes} id={article_id} type=\"article\"/>\n                    {/* {(article.author === 'butter_bridge' ? <button onClick={() => this.deleteArticle(article.article_id)}>Delete Article</button> : <p></p>)} */}\n                </ArticleHolder>\n                <Comments id={article_id}/>\n                </>\n            );\n        }\n    }\n}\n\nexport default SingleArticle;","/home/leahjwoodhead/my-news-app/src/components/ErrorMessage.js",[],"/home/leahjwoodhead/my-news-app/src/components/Comments.js",[],"/home/leahjwoodhead/my-news-app/src/components/CommentCard.js",[],"/home/leahjwoodhead/my-news-app/src/components/PostComment.js",[],"/home/leahjwoodhead/my-news-app/src/components/Voter.js",[],"/home/leahjwoodhead/my-news-app/src/components/SortCommentsForm.js",[],"/home/leahjwoodhead/my-news-app/src/components/Footer.js",[],"/home/leahjwoodhead/my-news-app/src/components/PostArticle.js",["74"],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":4,"column":10,"nodeType":"81","messageId":"82","endLine":4,"endColumn":23},{"ruleId":"79","severity":1,"message":"83","lin